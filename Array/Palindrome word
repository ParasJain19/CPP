#include <iostream>
using namespace std;
int main()
{
    int n,flag=1;
    cin>>n;
    char arr[n+1];
    cin>>arr;
    for(int i=0;i<n;i++)
    {
        if(arr[i] != arr[n-1-i])
        {
         flag=0;
         break;
        }
    }
    if(flag==0) cout<<"Not a Palindrome";
    else cout<<"Palindrome Word";
    return 0;
}
