#include<iostream>
using namespace std;
int main()
{  int n;
    cout<<"Enter the size of array:"; 
    cin>>n;
    int i,max,min,a[n];
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    max=a[0];
    min=a[0];
    for(i=0;i<n;i++)
    {
        if(max<a[i])
        {
            max=a[i];
        }
        else if(min>a[i])
        {
            min=a[i];
        }
        else{
            continue;
        }
        
    }
    cout<<"Max and min is "<<max<<"& "<<min;
}
