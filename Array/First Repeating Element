#include<iostream>
using namespace std;
int main()
{
     int n;
    cout<<"ENter the size of array:";
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }int i;
    for(i=0 ;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            {
                cout<<"Index "<<i+1;
                return 0;
            }
        }
    }if(i==n)
    {
        cout<<"-1";     
    
    }
    
}
