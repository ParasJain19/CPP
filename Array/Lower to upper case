#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int main()
{
    string str="jai shree ram jai shree balaji";
    ////CONVERT TO UPPER CASE
    for(int i=0;i<str.size();i++)
    {
        if(str[i]>='a' && str[i]<='z')
        {
            str[i]-=32;
        }
        cout<<str[i];
    }cout<<endl;
    //CONVERT TO LOWER CASE
       for(int i=0;i<str.size();i++)
    {
        if(str[i]>='A' && str[i]<='Z')
        {
            str[i]+=32;
        }
        cout<<str[i];
    }
    
}
