#include <iostream>
using namespace std;
int main()
{
     int n;
    cout<<"Enter the size of array:"; 
    cin>>n;
    int i,key,a[n];
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"ENter the key you want to find:";
    cin>>key;
    for(i=0;i<n;i++)
    {
      if(key==a[i])
      {cout<<"Key is at index  "<<i;
        break;
        return 0;
      }
      
    }
    if(i==n)
    {
        cout<<"-1";
    } 
}
